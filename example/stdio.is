// asm "extern printf";

fn putchar: void = (c: char) {
	asm "	push QWORD [rbp+16]
	mov rax, 1
	mov rdi, 1
	mov rdx, 1
	mov rsi, rsp
	syscall
	add rsp, 8";
}

fn strlen: int = (str: string) {
	asm "	mov rsi, qword [rbp+16]
	mov rax, -1
.looper:
	inc rax
	cmp byte [rsi+rax], 0x00
	jne .looper";
}

fn print: void = (str: string) {
	let i: int = 0;
	while (str[i]) {
	 	putchar(str[i]);
	 	i = i + 1;
	}
}

fn println: void = (str: string) {
	print(str);
	putchar(0xa);
}
